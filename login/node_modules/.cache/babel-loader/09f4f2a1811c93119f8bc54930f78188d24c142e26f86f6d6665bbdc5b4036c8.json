{"ast":null,"code":"export default {\n  name: \"order\",\n  data() {\n    return {\n      \"type\": \"\",\n      \"hascurrent\": false,\n      \"allorder\": [],\n      \"currentorder\": {}\n    };\n  },\n  methods: {\n    gettype() {\n      this.type = this.$route.query.type;\n      console.log(this.type);\n    },\n    getall() {\n      var token = localStorage.getItem(\"token\");\n      var send_data = {\n        \"token\": token\n      };\n      var allorder = [];\n      this.axios({\n        method: \"GET\",\n        url: `https://mock.apifox.cn/m1/2726825-0-default/client/index/order`,\n        data: JSON.stringify(send_data)\n      }).then(response => {\n        console.log(response);\n        if (true) {\n          // console.log(\"请求的返回值\")\n          // console.log(response.data.data.order_array)\n          this.allorder = response.data.data.order_array;\n        } else {\n          alert(\"response.msg\");\n        }\n      }).then(error => {\n        alert(\"出错了\");\n      });\n    },\n    getcurrent() {\n      var token = localStorage.getItem(\"token\");\n      var send_data = {\n        \"token\": token\n      };\n      this.axios({\n        method: \"GET\",\n        url: `https://mock.apifox.cn/m1/2726825-0-default/client/index/order`,\n        data: JSON.stringify(send_data)\n      }).then(response => {\n        console.log(response);\n        if (true) {\n          // console.log(\"请求的返回值\")\n          // console.log(response.data.data.order_array)\n          var allorder = response.data.data.order_array;\n        } else {\n          alert(\"response.msg\");\n        }\n      }).then(error => {\n        alert(\"出错了\");\n      });\n    },\n    gethistory() {\n      var allorder = this.getall();\n      this.orders = allorder.filter(function (data) {\n        return data.state == \"FINISHED\";\n      });\n    }\n  },\n  watch: {\n    $route() {\n      console.log(\"watch\");\n      this.gettype();\n      // 查看当前是否有正在执行的订单\n      if (this.type == \"charge